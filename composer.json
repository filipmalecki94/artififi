{
    "license": "proprietary",
    "type": "project",
    "require": {
        "php": ">=8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/dbal": "^3",
        "doctrine/doctrine-bundle": "^2.12",
        "doctrine/doctrine-migrations-bundle": "^3.3",
        "doctrine/orm": "^3.2",
        "johnpbloch/wordpress": "^6.5",
        "phpdocumentor/reflection-docblock": "^5.4",
        "phpstan/phpdoc-parser": "^1.29",
        "phpsword/sword-bundle": "^1.1",
        "symfony/asset": "~6.4.0",
        "symfony/asset-mapper": "~6.4.0",
        "symfony/console": "~6.4.0",
        "symfony/doctrine-messenger": "~6.4.0",
        "symfony/dotenv": "~6.4.0",
        "symfony/expression-language": "~6.4.0",
        "symfony/flex": "^2",
        "symfony/form": "~6.4.0",
        "symfony/framework-bundle": "~6.4.0",
        "symfony/http-client": "~6.4.0",
        "symfony/intl": "~6.4.0",
        "symfony/mailer": "~6.4.0",
        "symfony/mime": "~6.4.0",
        "symfony/monolog-bundle": "^3.0",
        "symfony/notifier": "~6.4.0",
        "symfony/process": "~6.4.0",
        "symfony/property-access": "~6.4.0",
        "symfony/property-info": "~6.4.0",
        "symfony/runtime": "~6.4.0",
        "symfony/security-bundle": "~6.4.0",
        "symfony/serializer": "~6.4.0",
        "symfony/stimulus-bundle": "^2.17",
        "symfony/string": "~6.4.0",
        "symfony/translation": "~6.4.0",
        "symfony/twig-bundle": "~6.4.0",
        "symfony/ux-turbo": "^2.17",
        "symfony/validator": "~6.4.0",
        "symfony/web-link": "~6.4.0",
        "symfony/yaml": "~6.4.0",
        "twig/extra-bundle": "^2.12 || ^3.0",
        "twig/twig": "^2.12 || ^3.0",
        "wpackagist-plugin/akismet": "^5.3",
        "wpackagist-theme/twentytwentyfour": "^1.1"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.42",
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^9.5",
        "roave/security-advisories": "dev-latest",
        "symfony/browser-kit": "~6.4.0",
        "symfony/css-selector": "~6.4.0",
        "symfony/debug-bundle": "~6.4.0",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "^7.1",
        "symfony/stopwatch": "~6.4.0",
        "symfony/web-profiler-bundle": "~6.4.0"
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org",
            "only": [
                "wpackagist-plugin/*",
                "wpackagist-theme/*"
            ]
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "composer/package-versions-deprecated": true,
            "ergebnis/composer-normalize": true,
            "johnpbloch/wordpress-core-installer": true,
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "sort-packages": true
    },
    "extra": {
        "installer-paths": {
            "wp/content/plugins/{$name}/": [
                "type:wordpress-plugin"
            ],
            "wp/content/themes/{$name}/": [
                "type:wordpress-theme"
            ]
        },
        "symfony": {
            "allow-contrib": "true",
            "require": "6.4.*"
        },
        "wordpress-install-dir": "wp/core"
    },
    "scripts": {
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd"
        }
    }
}
